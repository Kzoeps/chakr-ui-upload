{
  "name": "chakra-ui-upload",
  "description": "Chakra UI Upload component",
  "keywords": [
    "chakra-ui",
    "chakra-ui upload",
    "react"
  ],
  "homepage": "https://github.com/Kzoeps/chakra-ui-upload",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "github:Kzoeps/chakra-ui-upload"
  },
  "private": false,
  "version": "0.0.19",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/ChakraUpload.js",
  "module": "dist/ChakraUpload.umd.cjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ChakraUpload.js",
      "require": "./dist/ChakraUpload.umd.js"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublish": "pnpm build",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "classnames": "^2.3.2"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^2.7.1",
    "@emotion/react": "^11.1",
    "@emotion/styled": "^11.1",
    "framer-motion": "^10.1",
    "react": "^18.1",
    "react-dom": "^18.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^22.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6"
  }
}
